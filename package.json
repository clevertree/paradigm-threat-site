{
  "name": "paradigm-threat-site",
  "version": "2.0.3",
  "private": true,
  "scripts": {
    "autogen": "node server/autogen.js;",
    "dev": "next dev",
    "predev": "npm run autogen",
    "prebuild": "npm run autogen",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 8081",
    "cypress:component": "cypress run --component",
    "cypress:e2e": "cypress run --e2e",
    "cypress:e2e:prod": "CYPRESS_BASE_URL=http://localhost:8081 cypress run --e2e",
    "cypress:open": "cypress open",
    "cypress-prod:run": "CYPRESS_BASE_URL=https://paradigmthreat.net cypress run",
    "lint": "next lint",
    "deploy": "sh scripts/deploy.sh",
    "standard": "npx standard --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vercel/analytics": "^1.1.1",
    "@vercel/speed-insights": "^1.0.4",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^14.0.4",
    "@types/node": "^20.10.7",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "markdown-to-jsx": "^7.3.2",
    "mime": "^3.0.0",
    "next": "v13.5.5-canary.16",
    "postcss": "^8.4.29",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.67.0",
    "sharp": "^0.32.6",
    "simple-git": "^3.19.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/mime": "^3.0.1",
    "cypress": "^13.3.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.15.1",
    "husky": "^8.0.3",
    "standard": "^17.1.0"
  },
  "standard": {
    "globals": [
      "fetch",
      "expect",
      "describe",
      "context",
      "it",
      "cy"
    ]
  },
  "ignorePatterns": [
    "**/*.cy.js"
  ]
}
